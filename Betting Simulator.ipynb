   "source": [
    "# gets breakeven winning % from American odds\n",
    "# e.g. 52.4% for a -110 wager\n",
    "def breakeven(odds):\n",
    "    if odds < 0:\n",
    "        return round(-odds/(100 - odds), 3)\n",
    "    else:\n",
    "        return round(100/(100 + odds), 3)"
   ]
